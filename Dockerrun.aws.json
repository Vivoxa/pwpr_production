{
  "AWSEBDockerrunVersion": "2",
  "volumes": [
    {
      "name": "app",
      "host": {
        "sourcePath": "/var/app/current/app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "pwpr_app",
      "image": "084857879641.dkr.ecr.eu-west-1.amazonaws.com/pwpr:v0.1.12",
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "app",
          "containerPath": "/var/www/html",
          "readOnly": true
        }
      ]
    }
  ]
}
